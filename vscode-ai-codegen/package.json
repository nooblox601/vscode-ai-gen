{
  "name": "vscode-ai-codegen",
  "displayName": "VSCode AI Code Generator",
  "description": "An extension that generates code and frameworks using artificial intelligence.",
  "version": "0.1.0",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onCommand:extension.generateCode",
    "onCommand:extension.generateFramework"
  ],
  "main": "./src/extension.ts",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateCode",
        "title": "Generate Code"
      },
      {
        "command": "extension.generateFramework",
        "title": "Generate Framework"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "aiCodeGenView",
          "name": "AI Code Generator"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^4.4.3",
    "vscode": "^1.60.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}